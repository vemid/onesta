                                                    <div class="col-xs-12 px-0 d-flex flex-sm-row flex-column justify-content-start">
                                                        <label class="btn btn-md btn-default ml-50 mb-50 mb-sm-0 cursor-pointer"
                                                               for="account-upload">{{ t('Upload new photo') }}</label>
                                                        <input type="file" id="account-upload" class="hidden">
                                                        <button id="save-profile-img" class="btn btn-md btn-default ml-50 hidden">{{ t('Save image') }}</button>
                                                        <button id="reset-preview" class="btn btn-md btn-secondary ml-50 hidden">{{ t('Reset') }}</button>
                                                    </div>
